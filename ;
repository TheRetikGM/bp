@startuml
hide circle

class RuleSet {
    + matches(s : <color:green>String</color>) -> <color:purple>bool</color>
}

class Rule {
    + left : <color:green>String</color>
    + right : <color:green>String</color>
    + p : <color:blue>float</color>
    + matches(s : <color:green>String</color>) -> <color:purple>bool</color>
}

RuleSet *-down- "*" Rule

@enduml
